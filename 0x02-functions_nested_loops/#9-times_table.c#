#include "main.h"
/**
 *jack_bauer - gives the absolute value of a number
 *
 * Return: returns nothing
*/
void jack_bauer(void)
{
	int min;
	int hr;

	for (hr = 0; hr <= 23; hr++)
	{
		for (min = 0; min <= 59; min++)
		{
			_putchar((hr / 10) + 48);
			_putchar((hr % 10) + 48);
			_putchar(':');
dd			_putchar((min / 10) + 48);
			_putchar((min % 10) + 48);
			_putchar('\n');
		}
	}
}

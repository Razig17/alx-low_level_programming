#include "main.h"

/**
 * main - tests function that prints if integer is positive or negative
 * Return: 0
 */

char *create_array(unsigned int size, char c)
{
	char *s;
	int i;

	if (size == 0)
		return (NULL);
	s = malloc(sizeof(char) * size);
	i = 0;
        while (s[i])
	{
		s[i] = c;
		i++;
		)

	return (s);
}
